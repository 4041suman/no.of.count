import java.util.Scanner;

public class { noofcount
    public static void main(String[] args) {
        String rev="";
          Scanner sc=new Scanner(System.in);
          System.out.println("Enter an number : ");
          int num=sc.nextInt();
          int sum=0;
          while(num>0){
              sum=sum+num%10;
              num=num/10;
          }
          System.out.println(sum);
    }
}
